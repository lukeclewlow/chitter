
  <% if current_user %>

  <div id="new-peep">
    <form action="/peeps" method="post">
      Peep: <input type="text" name="peep">
      <input type="submit" value="Add Peep">
    </form>
  </div>
         
  <% end %>

<ul class="peeps">
  <% @peeps.each do |peep| %>
    <div id="peep_container"><li class="peeps"><%= peep.text %></li></div>
  <% end %>
</ul> 

</html>