
  <% if current_user %>

  <div id="new-peep">
    <form action="/peeps" method="post">
      Peep: <input type="text" name="peep">
      <input type="submit" value="Add Peep">
    </form>
  </div>
         
  <% end %>

<ul class="peeps">
  <% @peeps.reverse.each do |peep| %>
    <div id="peep_container"><li class="peeps"><%= peep.text %><div id="time">Created on: <%= peep.created_at.strftime("%d/%m/%Y at %I:%M%p") %></div></li></div>
  <% end %>
</ul> 

</html>